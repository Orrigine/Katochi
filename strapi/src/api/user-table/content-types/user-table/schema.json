{
  "kind": "collectionType",
  "collectionName": "user_tables",
  "info": {
    "singularName": "user-table",
    "pluralName": "user-tables",
    "displayName": "katochi users",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "lastName": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "username": {
      "type": "string",
      "unique": true
    },
    "email": {
      "type": "email",
      "unique": true
    },
    "password": {
      "type": "string"
    },
    "superAdmin": {
      "type": "boolean",
      "default": false
    },
    "redacteur": {
      "type": "boolean",
      "default": false
    },
    "otakeur": {
      "type": "boolean",
      "default": false
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event-table.event-table",
      "mappedBy": "users"
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "points": {
      "type": "biginteger"
    },
    "otakultureWins": {
      "type": "integer"
    },
    "achievementNumber": {
      "type": "integer"
    }
  }
}
